{
  "multipart": [
    {
      "when": { "AND": [  {"facing": "west"} ]},
      "apply": { "model": "mcwbiomesoplenty:block/parapet/dead_plank_parapet", "y": 180}
    },
    {
      "when": { "AND": [  {"facing": "north"} ]},
      "apply": { "model": "mcwbiomesoplenty:block/parapet/dead_plank_parapet", "y": 270}
    },
	{
      "when": { "AND": [  {"facing": "east"} ]},
      "apply": { "model": "mcwbiomesoplenty:block/parapet/dead_plank_parapet"}
    },
	{
      "when": { "AND": [  {"facing": "south"} ]},
      "apply": { "model": "mcwbiomesoplenty:block/parapet/dead_plank_parapet", "y": 90}
    },
    {
      "when": { "AND": [  {"facing": "west","part": "flower"} ]},
      "apply": { "model": "mcwbiomesoplenty:block/parapet/dead_plank_support", "y": 180}
    },
    {
      "when": { "AND": [  {"facing": "north","part": "flower"} ]},
      "apply": { "model": "mcwbiomesoplenty:block/parapet/dead_plank_support", "y": 270}
    },
	{
      "when": { "AND": [  {"facing": "east","part": "flower"} ]},
      "apply": { "model": "mcwbiomesoplenty:block/parapet/dead_plank_support"}
    },
	{
      "when": { "AND": [  {"facing": "south","part": "flower"} ]},
      "apply": { "model": "mcwbiomesoplenty:block/parapet/dead_plank_support", "y": 90}
    }
    
  ]
}


                                                            
