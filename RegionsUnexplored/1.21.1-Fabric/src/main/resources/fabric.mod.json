{
	"schemaVersion": 1,
  "id": "${archives_base_name}",
  "version": "${version}",

  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "homepage": "${mod_project_url}",
    "issues": "${mod_issue_tracker}"
  },

  "license": "${mod_license}",
  "icon": "logo.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "${mod_init}"
    ],
    "client": [
      "${mod_init_client}"
    ],
    "fabric-datagen": [
      "${mod_init_datagen}"
    ]
  },
  "depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*",
        "addonslib": ">=1.21.1-1.12",
        "regions_unexplored": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}