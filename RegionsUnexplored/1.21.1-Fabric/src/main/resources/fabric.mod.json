{
	"schemaVersion": 1,
  "id": "${archives_base_name}",
  "version": "${version}",

  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "${mod_license}",
  "icon": "logo.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "${mod_init}"
    ],
    "client": [
      "${mod_init_client}"
    ],
    "fabric-datagen": [
      "${mod_init_datagen}"
    ]
  },
  "depends": {
		"fabricloader": ">=0.17.3",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}