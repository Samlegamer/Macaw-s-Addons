{
  "schemaVersion": 1,
  "id": "${archives_base_name}",
  "version": "${version}",

  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "${mod_license}",
  "icon": "logo.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "${mod_init}"
    ],
    "client": [
      "${mod_init_client}"
    ]
  },
  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.18.2",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
